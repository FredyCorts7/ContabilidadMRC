@page
@model ContabilidadMRC.Pages.Reports.ExpirationDateModel
@{
    ViewData["Title"] = "ExpirationDate";
}

<h1>ExpirationDate</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Product[0].Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Product[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Product[0].TypeProductID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Product[0].BrandID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Product[0].Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Product[0].Stock)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Product)

        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Code)

                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TypeProduct.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Brand.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Price)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Stock)
                </td>

            </tr>
        }
    </tbody>
</table>